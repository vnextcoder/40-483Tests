<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <staticContent>
            <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="15.00:00:00" />
        </staticContent>
        <httpProtocol>
            <customHeaders>
                <add name="X-UA-compatible" value="IE=Edge,chrome=1" />
            </customHeaders>
        </httpProtocol>
    </system.webServer>
</configuration>
